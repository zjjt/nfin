backgroundWhite()
  background -moz-linear-gradient(top,  rgba(205,154,46,1) 0%, rgba(205,154,46,1) 54%, rgba(205,154,46,1) 66%, rgba(205,154,46,0.53) 82%, rgba(205,154,46,0) 100%)
  background -webkit-linear-gradient(top,  rgba(205,154,46,1) 0%,rgba(205,154,46,1) 54%,rgba(205,154,46,1) 66%,rgba(205,154,46,0.53) 82%,rgba(205,154,46,0) 100%) /* Chrome10-25,Safari5.1-6 */
  background linear-gradient(to bottom,  rgba(205,154,46,1) 0%,rgba(205,154,46,1) 54%,rgba(205,154,46,1) 66%,rgba(205,154,46,0.53) 82%,rgba(205,154,46,0) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */



html
  width 100%
  height 100%
body
  margin 0
  width 100%
  height 100%
  background url('/img/background.jpg');
  background-attachment fixed
  background-repeat no-repeat
  background-size cover
  font-family Roboto, sans-serif;

.okMsg
  text-align center
footer
  text-align center
  color white
  
.field
  border-left 1px dashed gray
  border-top 1px dashed gray
  border-right 1px dashed gray
  padding-left 10px
  padding-right 10px



.twoWindows
  display flex
  flex-direction row
  justify-content space-around
  .VerticalSeparator
    width 15px
    background-color #999999
    min-height 100%
  .window
    flex-grow 1
    max-width 50%
    
.loadmoreDiv
  padding 15px
  display flex
  justify-content center
  align-items center
.helperDiv
  padding 10px
  background gray  
.cardMenu
  width 100%
  display flex
  flex-wrap wrap
  margin-top -50px
  justify-content center
  .cardContainer
    width 250px
    height 150px
    margin-top 190px
    margin-left 4%
    margin-right 4%
    .cards
      width 100%
      height 100%
      transition all .2s ease-in-out
      &:hover
        cursor pointer
        transform scale(1.1)
      .cardsHeader div
        padding-right 0px !important
        width 100%
        text-align center

.inAppBtnDiv
  display flex
  justify-content flex-end
  margin-top 6%
 

.formDiv
  padding-left 30px
  padding-right 30px
  display flex
  flex-direction column
  justify-content center
  align-items center
.icono
  &:hover
    cursor pointer


.datepicker 
   border none  
.hidden
  visibility hidden
.errorMsg
  color red
.generalSection
  display flex
  flex-direction column
  flex-grow 2
.centeredContentSingle
  width 100%
  min-height 600px
  flex-grow 1
  height 100%
  display flex
  
  .contentWrapper
    width 960px
    min-height 600px
    height auto
    position relative
    background-color white
    flex-grow 1
    .optionTabs
      position absolute
      bottom 0
      width 100%
.warningMsg
  color #cc992c
.topaligned
  display flex
  flex-direction row
  justify-content space-around
  align-items center
  flex-wrap wrap
.centeredContent
  width 100%
  min-height 630px
  height 100%
  display flex
  justify-content center
  align-items center
  .loginDiv
    width 40%;
    min-height 200px
    margin auto
    background-color white
    display flex
    flex-direction column
    justify-content center
    align-items center
    .loginformCont1
      display flex
      flex-direction column
      .loginBtnDiv
        display flex
        justify-content center
        padding 10px
    .loginformCont
      width 100%
      flex-grow 2
      display flex
      flex-direction column
      .loginBtnDiv
        display flex
        justify-content center
        padding 10px




.uploadDiv
  display flex
  flex-direction column
  justify-content center
  align-items center
  
.uploadDiv div
  display flex
  justify-content center

 
.masterContainer
  width:100%
  min-height 900px
  height auto
  display flex
  flex-grow 1
  flex-direction column
  position relative

.headerContainer
  display: flex
  flex-grow 1
  justify-content space-between
  align-items flex-start
  backgroundWhite()
  min-height 30px
  height: auto
  padding 10px
  z-index 10
  @media screen and (max-height: 250px)
    min-height 50px
    height 50px
  .logo
    width 100px
    height 50px
    @media screen and (max-height: 250px)
      width 70px
      height 35px

.headerDate
  color #212f68
  margin-left -60px 
  margin-top 0px